{
  "name": "econexo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "export": "next build",
    "start": "next start",
    "lint": "eslint",
    "db:link": "make supabase-link PROJECT_REF=$PROJECT_REF",
