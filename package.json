    "mobile:run:ios": "npm run mobile:build && npm run mobile:sync && npx cap run ios"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.3",
    "@capacitor/camera": "^7.0.2",
    "@capacitor/cli": "^7.4.3",
